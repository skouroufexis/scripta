,GAAA,KAAA,CAAA,QAAA,CAAA,CAAA,EAAA,IAAA;AACA,QAAA,QAAA,GAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,YAAA,KAAA,GAAA,QAAA,CAAA,CAAA,CAAA;AACA,YAAA,OAAA,GAAA,QAAA,CAAA,CAAA,CAAA;AACA,YAAA,OAAA,GAAA,KAAA,GAAA,GAAA,GAAA,OAAA;AACA,eAAA,OAAA;AACA,OARA,EAtBA;AAgCA,MAAA,KAAA,EAAA,YAAA;AACA,eAAA,KAAA,CAAA,QAAA,CAAA,CAAA,EAAA,KAAA;AACA,OAFA,EAhCA;AAoCA,MAAA,IAAA,EAAA,YAAA;AACA,eAAA,KAAA,CAAA,QAAA,CAAA,CAAA,EAAA,IAAA;AACA,OAFA,EApCA;AAuCA,MAAA,IAAA,EAAA;AAvCA,KAAA;AA2CA,GA/CA;AAiDA,EAAA,KAAA,EAAA,CAAA,UAAA,CAjDA;AAkDA,EAAA,OAAA,EAAA;AAEA,IAAA,MAAA,EAAA,kBAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,KAJA;AAKA,IAAA,OAAA,EAAA,mBAAA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,KAPA;AASA,IAAA,SAAA,EAAA,qBAAA;AACA,UAAA,KAAA,GAAA,QAAA,CAAA,cAAA,CAAA,iBAAA,EAAA,KAAA,CAAA,IAAA,EAAA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,KAZA;AAcA,IAAA,QAAA,EAAA,oBAAA;AACA,UAAA,IAAA,GAAA,QAAA,CAAA,cAAA,CAAA,mBAAA,EAAA,KAAA,CAAA,IAAA,EAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,KAjBA;AAkBA,IAAA,GAAA,EAAA,eAAA;AAMA,UAAA,aAAA,GAAA;AAAA,QAAA,OAAA,EAAA,CACA;AAAA,UAAA,IAAA,EAAA,KAAA,KAAA;AAAA,UAAA,QAAA,EAAA;AAAA,SADA,EAGA,KAAA,IAHA;AAAA,OAAA,CANA,CAYA;;AACA,MAAA,OAAA,CAAA,SAAA,CAAA,aAAA,EAAA,IAAA;AAIA,KAnCA,CAsCA;;AAtCA;AAlDA,CAAA","sourcesContent":["<template>\n\n    <div id=\"Note\" class=\"container\">\n        <div class=\"row\"><button v-on:click='$emit(\"back_notes\")'><i class=\"fas fa-caret-left back\"></i></button></div>\n        \n\n        <div class=\"container\">\n\n            <div class=\"row\">\n                <div class=\"col-6\"></div>\n                <div v-if=\"!edit\" class=\"col darkcyan\"><b>{{this.time}}</b></div>\n                <button v-if=\"edit\" class=\"col\" >{{this.time}}</button>\n\n                <div v-if=\"!edit\" class=\"col darkcyan\"><b> {{this.date}}</b> </div>   \n                <button v-if=\"edit\" class=\"col\" >{{this.date}}</button>\n            </div>\n            <div v-if=\"!edit\" class=\"row darkcyan background_darkcyan top1\"><b>{{this.title}}</b></div>\n            <input id=\"note_inputTitle\" v-on:keyup='editTitle' v-if=\"edit\" type=\"text\" class=\"row top1\" v-bind:value='this.title' >\n\n            <div id=\"note_divBody\" v-if=\"!edit\" class=\"row black top1\">{{this.body}}             \n            </div>\n            <textarea  class=\"row top1\" id='note_textareaBody' v-if=\"edit\" maxlength=\"6000\"\n                        v-bind:value='this.body' v-on:keyup='editBody'\n            >                \n            </textarea>\n            \n            <div v-if=\"edit\" class=\"row top1\">\n                <button v-on:click='editOff' class=\"left-auto\">Done</button>                              \n            </div>    \n\n            <div v-if=\"!edit\" class=\"row top1\">\n                <button class=\"left-auto\" v-on:click='pdf'><i class=\"fas fa-file-pdf\"></i></button>                  \n                <button class=\"left2 \" v-on:click='editOn'><i class=\"far fa-edit\"></i></button>\n            </div>    \n\n            \n        </div>\n    </div>    \n    \n</template>\n\n\n<script>\n\nimport pdfMake from \"pdfmake/build/pdfmake\";\nimport pdfFonts from \"pdfmake/build/vfs_fonts\";\npdfMake.vfs = pdfFonts.pdfMake.vfs;\n\nexport default {\n    name:'Note',\n\n    data:function(){\n        return{\n           date:(()=>{\n\n               let months=['Jan','Feb','Mar','Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec'];\n\n               let fullDate=this.noteData[0].date;\n                   fullDate=fullDate.split('-');\n               \n               let year=fullDate[0];\n               let month=fullDate[1];\n                   months.forEach((m,index)=>{\n                       if(month==index+1 || month== '0'+index+1){\n                           month=m;\n                       } \n                   });\n               let day=fullDate[2];\n                   day=day.substring(0,2);\n\n               let newDate=day+' '+ month+' '+year;\n                return newDate;\n           })(),\n\n           time:(()=>{\n                \n                let fullTime=this.noteData[0].time;\n                    fullTime=fullTime.split(':');\n                let hours=fullTime[0];\n                let minutes=fullTime[1];\n                let newTime=hours+':'+minutes;\n                return newTime;\n           })(),\n\n           title:(()=>{\n                return this.noteData[0].title;    \n           })(),\n\n           body:(()=>{\n                return this.noteData[0].body;    \n           })(),\n           edit:false,\n\n           \n        }\n    },\n\n    props:['noteData'],\n    methods:{\n        \n        editOn:function(){\n            this.edit=true;\n        },\n        editOff:function(){\n            this.edit=false;\n        },\n\n        editTitle:function(){\n            let title=document.getElementById('note_inputTitle').value.trim();\n            this.title=title;\n        },\n\n        editBody:function(){\n            let body=document.getElementById('note_textareaBody').value.trim();\n            this.body=body;\n        },\n        pdf: function(){\n            \n            \n        \n        \n        \n        var docDefinition = { content: [\n                            {text:this.title,fontSize:15},\n                            \n                            this.body\n                            \n                            ] };\n        // pass the \"win\" argument\n        pdfMake.createPdf(docDefinition).open();\n            \n            \n            \n        },\n        \n\n       //\n\n    },\n}\n</script>\n\n\n<style>\n\n\n    #note_divBody{\n        position: relative;\n        text-align: justify;\n        white-space: pre-wrap;\n        height: 200px;\n        overflow-y: scroll;\n        padding: 1%;\n    }\n    \n   #note_textareaBody {\n        \n        resize: none;\n        text-align: justify;\n        width: 100%;                \n        height: 200px;\n        border: solid rgba(0,0,0,0.1) thin;\n        color: rgba(0,0,0,0.8);\n        \n        /* overflow-y: scroll; */\n        padding: 1%;\n    }\n\n    \n\n</style>\n\n"],"sourceRoot":"src/components"}]}
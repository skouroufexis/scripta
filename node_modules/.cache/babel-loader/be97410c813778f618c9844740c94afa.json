BAAA,mDAAA;AACA,SAHA,MAIA;AACA,cAAA,SAAA,CAAA,OAAA,CAAA,GAAA,KAAA,CAAA,EAAA;AACA,iBAAA,QAAA,CAAA,CAAA;AACA,kBAAA,sCAAA;AACA;AACA;;AAEA,YAAA,SAAA,IAAA,SAAA,EAAA;AACA,eAAA,QAAA,CAAA,CAAA;AACA,gBAAA,uDAAA;AACA;;AAEA,aAAA,QAAA,GAAA,KAAA,SAAA,CAzCA,CA4CA;;AACA,YAAA,OAAA,GAAA,IAAA,cAAA,EAAA;AACA,QAAA,OAAA,CAAA,IAAA,CAAA,MAAA,EAAA,kCAAA;AAEA,YAAA,IAAA,GAAA,IAAA,CAAA,SAAA,CAAA;AAAA,UAAA,SAAA,EAAA,KAAA,QAAA;AAAA,UAAA,UAAA,EAAA,KAAA,KAAA;AAAA,UAAA,aAAA,EAAA,KAAA;AAAA,SAAA,CAAA;AACA,QAAA,OAAA,CAAA,IAAA,CAAA,IAAA;AAEA,YAAA,IAAA,GAAA,IAAA;;AACA,QAAA,OAAA,CAAA,kBAAA,GAAA,YAAA;AACA,cAAA,KAAA,UAAA,IAAA,CAAA,IAAA,KAAA,MAAA,IAAA,GAAA,EAAA;AAGA,YAAA,KAAA,CAAA,KAAA,YAAA,CAAA;;AAEA,gBAAA,KAAA,YAAA,IAAA,yBAAA,EAAA;AACA;AACA,cAAA,IAAA,CAAA,SAAA;AACA,aAHA,MAIA,IAAA,KAAA,YAAA,IAAA,sBAAA,EAAA;AACA;AACA,cAAA,IAAA,CAAA,QAAA,CAAA,CAAA;AACA;AAGA;AACA,SAjBA;AAmBA,OAvEA,CAwEA,OAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA;AACA;AAEA,KAnFA;AAqFA,IAAA,WAAA,EAAA,qBAAA,CAAA,EAAA;AACA;AACA,UAAA,WAAA,GAAA,QAAA,CAAA,sBAAA,CAAA,WAAA,CAAA;AACA,MAAA,WAAA,CAAA,CAAA,GAAA,CAAA,CAAA,CAAA,KAAA,CAAA,eAAA,GAAA,OAAA;;AAGA,UAAA,CAAA,IAAA,CAAA,EAAA;AACA,aAAA,QAAA,GAAA,QAAA,CAAA,cAAA,CAAA,UAAA,EAAA,KAAA;AACA,OAFA,MAGA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,aAAA,KAAA,GAAA,QAAA,CAAA,cAAA,CAAA,OAAA,EAAA,KAAA;AACA,OAFA,MAIA,IAAA,CAAA,IAAA,CAAA,EAAA;AAEA,YAAA,cAAA,GAAA,QAAA,CAAA,cAAA,CAAA,WAAA,CAAA;AAEA,aAAA,SAAA,GAAA,QAAA,CAAA,cAAA,CAAA,WAAA,EAAA,KAAA,CAJA,CAMA;;AACA,YAAA,KAAA,SAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,UAAA,cAAA,CAAA,QAAA,GAAA,KAAA;AACA,UAAA,cAAA,CAAA,KAAA,CAAA,eAAA,GAAA,OAAA;AACA,SAHA,MAIA;AACA,UAAA,cAAA,CAAA,QAAA,GAAA,IAAA;AACA,UAAA,cAAA,CAAA,KAAA,CAAA,eAAA,GAAA,YAAA;AACA,UAAA,cAAA,CAAA,KAAA,GAAA,EAAA;AACA;AAIA,OAnBA,MAqBA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,aAAA,SAAA,GAAA,QAAA,CAAA,cAAA,CAAA,WAAA,EAAA,KAAA;AACA;AACA,KA1HA;AA4HA;AACA,IAAA,QAAA,EAAA,kBAAA,CAAA,EAAA;AAEA,UAAA,WAAA,GAAA,QAAA,CAAA,sBAAA,CAAA,WAAA,CAAA;AAEA,MAAA,WAAA,CAAA,OAAA,CAAA,UAAA,CAAA,EAAA;AACA,YAAA,CAAA,KAAA,WAAA,CAAA,CAAA,CAAA,EAAA;AACA,UAAA,CAAA,CAAA,KAAA,CAAA,eAAA,GAAA,YAAA;AACA,SAFA,MAGA;AACA,UAAA,CAAA,CAAA,KAAA,CAAA,eAAA,GAAA,OAAA;AACA;AAEA,OARA;AASA,MAAA,WAAA,CAAA,CAAA,GAAA,CAAA,CAAA,CAAA,KAAA,CAAA,eAAA,GAAA,sBAAA;AACA,KA3IA;AA6IA,IAAA,SAAA,EAAA,qBAAA;AACA,WAAA,KAAA,CAAA,WAAA,EAAA,CAAA;AACA;AA/IA;AAbA,CAAA","sourcesContent":["<template>\n\n    <main id=\"Signup\" class=\"container\">\n        <div class=\"container\">\n            <div class=\"row header\">\n                <h3>User Registration</h3>\n            </div>\n\n            <form class=\"row\" action=\"#\">\n                <label class=\"col-12\" for=\"username\" v-on:keyup='updateInput(1)'>Username<br>\n                    <input type=\"text\" class=\"userEntry\"  id=\"username\">\n                </label>\n\n                <label class=\"col-12\" for=\"email\" v-on:keyup='updateInput(2)'>Email<br>\n                    <input type=\"text\" class=\"userEntry\"  id=\"email\">\n                </label>\n\n                <label class=\"col-12\" for=\"password1\" v-on:keyup='updateInput(3)'>Password<br>\n                    <input type=\"password\" class=\"userEntry\"  id=\"password1\" maxlength=\"20\">\n                </label>\n\n                <label class=\"col-12\" for=\"password2\" v-on:keyup='updateInput(4)'>Repeat password<br>\n                    <input type=\"password\" class=\"userEntry\"  id=\"password2\" maxlength=\"20\" disabled>                    \n                </label>\n\n                <button class=\"col-6\" type=\"button\" id=\"button_register\" v-on:click='register'>Register</button>\n\n            </form>\n\n            <div class=\"row\" id=\"container_button_back\">\n                \n                <button  v-on:click='gotoLogin' id=\"button_back\">Back</button>  \n                \n            </div>\n\n            \n        </div>\n\n\n    </main>    \n</template>\n\n\n<script>\n\n\nexport default {\n    name:'Signup',\n\n    data:function(){\n        return{\n            username:'',\n            email:'',\n            password1:'',\n            password2:'',\n            password:''\n        }\n    },\n\n    methods:{\n        register:function(){\n            let username=this.username;\n            let email=this.email;\n            let password1=this.password1;\n            let password2=this.password2;            \n            //validate user input\n            try{\n                \n                //empty input\n                if(username.trim()==''){\n                    this.setFocus(1);\n                    throw 'Please enter your username';\n                \n                }\n                if(email.trim()==''){\n                    this.setFocus(2);\n                    throw 'Please enter a valid email address';\n                }\n                else{\n                    //validate email format\n                    let emailValidator=require('email-validator');\n                    let isValid=emailValidator.validate(email);\n                    if(isValid==false){\n                        this.setFocus(2);\n                        throw 'Please enter a valid email address';\n                    }\n                }\n                if(password1.trim()==''){\n                    this.setFocus(3);    \n                    throw 'Please enter a valid password';\n                }\n                else if(password1.length<8){\n                    this.setFocus(3);\n                    throw 'Password must be between 8 and 20 characters long';\n                }\n                else{\n                    if(password1.indexOf(' ')>=0){\n                        this.setFocus(3);\n                        throw 'Password cannot contain empty spaces';\n                    }\n                }                \n\n                if(password1!=password2){\n                        this.setFocus(4);\n                    throw 'Passwords do not match. Please re-enter your password';\n                }\n                                \n                this.password=this.password1;\n                \n\n                //POST request with user credentials                    \n                let request= new XMLHttpRequest();    \n                request.open('POST','http://localhost:8000/addNewUser');\n\n                let data=JSON.stringify({user_name:this.username,user_email:this.email,user_password:this.password});\n                request.send(data);  \n                \n                let self=this;\n                request.onreadystatechange = function (){        \n                    if(this.readyState==4 && this.status==200){\n                    \n                    \n                    alert(this.responseText);\n\n                    if(this.responseText=='User successfully added'){\n                        //redirect to login page\n                        self.gotoLogin();\n                    }\n                    else if(this.responseText== 'Email already in use'){\n                        //highlight email field\n                        self.setFocus(2);\n                    }\n                    \n\n                    }\n                };\n\n            }\n            catch (error){\n                alert(error);\n            }\n\n        },\n\n        updateInput:function(n){\n            //remove highlight from input field\n            let userEntries=document.getElementsByClassName('userEntry');    \n            userEntries[n-1].style.backgroundColor='white';        \n           \n\n            if(n==1){\n               this.username=document.getElementById('username').value;                \n            }\n            else if (n==2) {\n               this.email=document.getElementById('email').value; \n            }\n\n            else if (n==3) {\n               \n               let passwordInput2=document.getElementById('password2');\n               \n               this.password1=document.getElementById('password1').value; \n               \n               //enable or disable the 2nd password field according to the 1st pw's length\n               if(this.password1.length>=8){\n                  passwordInput2.disabled=false;  \n                  passwordInput2.style.backgroundColor='white';\n               }\n               else{\n                  passwordInput2.disabled=true;   \n                  passwordInput2.style.backgroundColor='whitesmoke';\n                  passwordInput2.value='';\n               }\n               \n\n\n            }\n\n            else if (n==4) {\n               this.password2=document.getElementById('password2').value; \n            }\n        },\n\n        //function to set focus to input fields that weren't completed properly\n        setFocus:function(n){\n            \n            let userEntries=document.getElementsByClassName('userEntry');\n\n            userEntries.forEach(u => {\n                if(u===userEntries[3]){\n                    u.style.backgroundColor='whitesmoke';\n                }\n                else{\n                    u.style.backgroundColor='white';\n                }\n                \n            });            \n            userEntries[n-1].style.backgroundColor='rgb(255, 140, 0,0.5)';\n        },\n\n        gotoLogin:function(){\n            this.$emit('gotoLogin',0);\n        },\n    },\n}\n</script>\n\n\n<style>\n\n\n    .userEntry {\n                text-align: center;}\n\n    #password2 {background-color: whitesmoke;}\n    #button_register{\n        background-color: rgb(0, 139, 139); \n        color: white;        \n        height: 40px;\n        \n    }\n\n    #container_button_back {\n        \n        margin-top: 2%;\n        justify-content: center;\n    }\n    #button_back{\n        background-color: rgba(0,0,0,0);\n        \n        \n    }\n    \n\n</style>\n\n"],"sourceRoot":"src/components"}]}
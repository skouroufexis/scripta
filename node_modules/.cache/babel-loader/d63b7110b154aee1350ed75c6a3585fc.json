A,eAAA,KAAA,GAAA,IAAA;AACA;AARA;AAcA;AAhCA;AAtBA,CAAA","sourcesContent":["<template>\n\n  <div id=\"app\">\n    <div id=\"warning\" ><b><p>Please rotate your device in portrait position</p></b></div>    \n   <Header />\n   <Index v-if=\"isIndex\" />\n   <New v-if=\"isNew\" />\n   <Footer id=\"Footer\" v-on:navigation='navigation'  />\n  </div>\n</template>\n\n<script>\n\nimport Header from './components/Header';\nimport Index from './components/Index';\nimport Footer from './components/Footer';\nimport New from './components/New';\n\n\n\n\nexport default {\n  name: 'App',\n  components: {\n    Index,Header, Footer,New,\n    \n    \n    \n  },\n\n  data:function(){\n    return {\n      isNew:false,\n      isIndex:true,\n    }\n  },\n mounted(){\n\n   //set the background-colour of the come icon\n   let footerButtons=document.getElementsByClassName('button_main');\n   footerButtons[0].style.backgroundColor='rgb(0, 138, 138,0.1)';\n\n },\n  methods:{\n\n    \n    navigation:function(n){\n     \n    let footerButtons=document.getElementsByClassName('button_main');\n    let c;\n\n     this.isIndex=false;\n     this.isNew=false;\n\n    // change background-colour of currently active menu option\n    for(c=0;c<footerButtons.length;c++){\n        footerButtons[c].style.backgroundColor='white';\n    }\n\n    footerButtons[n-1].style.backgroundColor='rgb(0, 138, 138,0.1)';\n\n     switch(n){\n       case 1:\n         this.isIndex=true;\n        //  footerButtons[0].style \n         break;\n        \n        case 2:\n          this.isNew=true;\n          break;\n     }\n      \n\n\n      \n    },\n\n    \n  }\n\n}\n</script>\n\n<style>\nbody {\n      \n      height: 100vh;\n      background-image: url('./assets/background2.png');\n      background-size: contain;\n      background-position: center;\n      /* background-origin: content-box; */\n      \n      \n}\n\n#app {\n\n      margin-top: auto;\n      margin-bottom: auto;\n      \n       height: 100%;\n        /* height: 100vh; */\n        position: relative;\n        overflow-y: scroll;\n}\n\n\n#warning {\n            background-color: black; \n            color: white;\n            position: absolute; top: 0;\n            z-index: 1;\n            height: 100%;\n            width: 100%;\n            display: none;\n            justify-content: center;\n            align-items: center;\n  }\n\n\n\n#Header {\n         margin-top: 1%;\n         /* its height will be determined by the logo image  */\n         /* which is set to 10vh */\n        \n         height: auto; \n         \n         \n}\n\n\n#Footer  {\n          height: auto;\n\n          \n} \n\nmain { \n      \n      position: relative;\n      height: 70%;\n      \n      overflow-y: scroll;   \n\n\n      /* background-image: url('./assets/background2.png');\n      background-size: contain;\n      background-position: center; */\n}\n\n  input {\n    outline: none;\n  }\n\n  input:focus,textarea:focus {  \n                              outline: none;\n                             \n  }\n   \n button {\n         font-weight: bold;\n         border: none;\n         color: darkcyan;\n         \n         }\n\n button:focus{outline: none;\n              \n                }\n\n\n  \n\n\n\n/* @media screen and (max-height:10cm) {\n  #warning {\n    display: flex;\n  }\n\n  #Header {\n         display: none;\n}\n\n#Footer {display: none;}\nmain {display: none;}\n} */\n\n</style>\n"],"sourceRoot":"src"}]}
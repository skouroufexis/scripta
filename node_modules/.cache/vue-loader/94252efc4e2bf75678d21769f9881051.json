day.getHours();\n            let minutes = today.getMinutes();\n                if(minutes<10)\n                    {\n                        minutes= '0'+minutes;\n                    }\n            let time = hours +\":\"+ minutes;\n            return time;\n            \n          \n          },\n    day:function(){\n        let today= new Date();\n        let day=today.getDate();\n        let month=today.getMonth();\n\n        //month names\n        let months=['Jan','Feb','Mar','Apr','May','Jun','Jul','Aug','Set','Oct','Nov','Dec']\n\n        month=months[month];\n\n        let year = today.getFullYear();\n\n\n        \n\n\n        return day +' '+ month + ' '+ year;\n        \n        } \n    \n  },\n    \n}\n\n</script>\n\n\n<style>\n#title {\n        \n        border: none;\n        border-bottom: solid rgba(0,0,0,0.3) thin;\n        }\n#time, #day {\n             font-weight: bold;\n             text-align: center;   \n            cursor: pointer;}\n\n\n\n\n</style>"]}]}